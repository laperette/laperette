{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:css": "postcss src/styles/index.css -o src/index.css",
    "watch:css": "postcss src/styles/index.css -o src/index.css -w",
    "start-dev": "yarn watch:css & react-scripts start",
    "build": "yarn build:css && react-scripts build",
    "start": "serve -s ./build -l $PORT",
    "test": "NODE_ENV=test TZ=UTC react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@date-io/date-fns": "2.10.6",
    "@hookform/resolvers": "^1.0.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "@types/uuid": "^8.3.0",
    "axios": "^0.21.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "joi": "^17.3.0",
    "lodash": "^4.17.20",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.11.5",
    "react-router-dom": "^5.2.0",
    "serve": "^11.3.0",
    "styled-components": "^5.2.1",
    "swr": "^0.3.8",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/user-event": "^12.2.0",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.10",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.4",
    "autoprefixer": "^10.0.2",
    "mockdate": "^3.0.2",
    "mutationobserver-shim": "^0.3.7",
    "postcss": "^8.1.9",
    "postcss-cli": "^8.3.0",
    "react-scripts": "4.0.0",
    "tailwindcss": "^2.0.1",
    "typescript": "^4.0.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "workspaces": {
    "nohoist": [
      "@types/*",
      "jest"
    ]
  }
